      <path pathString="EmbeddedPy" checkConnect="true" seoe="false">
        <scriptName>EmbeddedScript</scriptName>
        <scriptText><![CDATA[#  Mission-day display = PC clock + 2 h (fixed CET offset)
#  Shows 0/17 before 21 Jul, 1/17 on 21 Jul … 17/17 on 6 Aug, then 17/17 thereafter.

from org.yamcs.studio.script import PVUtil
from java.time import Instant, ZoneOffset, LocalDate
from java.time.temporal.ChronoUnit import DAYS

pv = pvs[0]   # sys://time  (Unix-millis from PC)
if pv.getValue() is None:
    widget.setPropertyValue("text", "---")
else:
    # get PC ms +2 h → CET
    millis = PVUtil.getTimeInMilliseconds(pv)
    cet_ms = millis + 2 * 60 * 60 * 1000
    # current date in CET
    currentDate = Instant.ofEpochMilli(int(cet_ms)) \
                         .atOffset(ZoneOffset.UTC) \
                         .toLocalDate()

    # define mission window
    missionStart = LocalDate.of(2025, 7, 21)
    missionEnd   = LocalDate.of(2025, 8,  6)
    totalDays    = DAYS.between(missionStart, missionEnd) + 1

    # compute mission day:
    if currentDate.isBefore(missionStart):
        dayCount = 0
    elif currentDate.isAfter(missionEnd):
        dayCount = totalDays.intValue()
    else:
        # inclusive: July 21 → day 1
        dayCount = DAYS.between(missionStart, currentDate).intValue() + 1

    # format as "N/17"
    widget.setPropertyValue("text", "%d/%d" % (dayCount, totalDays))]]></scriptText>
        <pv trig="true">sys://time</pv>
      </path>
