        <scriptName>EmbeddedScript</scriptName>
        <scriptText><![CDATA[#  Space‐mission–style display = PC clock + 2 h (fixed offset)
#  Works in Yamcs Studio 5.x and in the Web runner

from org.yamcs.studio.script import PVUtil
from java.time import Instant, ZoneOffset
from java.time.format import DateTimeFormatter

# Day‐of‐year (3 digits), slash, hours:minutes:seconds
FMT = DateTimeFormatter.ofPattern("DDD/HH:mm:ss")

pv = pvs[0]                               # sys://time  (Unix‐millis from PC)
if pv.getValue() is None:                 # not yet connected → show placeholder
    widget.setPropertyValue("text", "---")
else:
    # get raw PC‐time ms, add fixed +2 h for CET
    millis = PVUtil.getTimeInMilliseconds(pv)
    cet_ms = millis 
    # interpret as an OffsetDateTime at UTC (so the +2 h is baked in)
    cet = Instant.ofEpochMilli(int(cet_ms)).atOffset(ZoneOffset.UTC)
    # format as "DDD/HH:mm:ss", e.g. "335/13:35:22"
    widget.setPropertyValue("text", cet.format(FMT))]]></scriptText>
        <pv trig="true">sys://time</pv>
      </path>
